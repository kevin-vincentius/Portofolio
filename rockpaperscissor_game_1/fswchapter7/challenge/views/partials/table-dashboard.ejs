<div class="container">
    <br>
    <h2 class="mt-4 text-center">List User</h2>
    <br>
    <table class="table table-striped table-light">
        <thead class="thead-dark">
        <tr>
            <th scope="col">No</th>
            <th scope="col">ID</th>
            <th scope="col">Username</th>
            <th scope="col">Role</th>
            <th scope="col">Action</th>

        </tr>
        </thead>
        <tbody>
        <% let i = 1; %>
        <% user_game.forEach((data) => { %>
            <% id = data.id %>
        <tr>
            <td scope="row"><%= i %></td>
            <td><%= data.id %></td>
            <td><%= data.username %></td>
            <td><%= data.role %></td>
            <td>
                <button type="button" class="btn btn-success"><a href="/dashboard/update/<%= data.id %>" class="text-light text-decoration-none"> Edit</a></button>
                <button type="button" class="btn btn-danger"><a href="/dashboard/delete/<%= data.id %>" class="text-light text-decoration-none"> Delete</a></button>
            </td>
        </tr>
        <% i++ %>
        <% }); %>
        </tbody>
    </table>
</div>
